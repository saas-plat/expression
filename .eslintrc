{
    "plugins": ["react"],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "Promise": true,
      "__DEV__": true,
      "__VERSION__": true,
      "expect": true,
      "describe": true,
      "it": true,
      "Symbol": true,
      "test": true
    },
    "rules":{
      "react/display-name": "off",
      "no-console": "off"
    }
}
